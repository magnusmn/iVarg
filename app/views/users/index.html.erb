<% provide(:title, 'All users') %>


<table width=100% class="table table-striped">
  <tr>
    <th>ID</td>
    <th>Name</td>
    <th>Email</td>
  </tr>

<% @user.each do |u| %>
  <tr>
    <td><%=link_to u.id, user_path(u)%></td>
    <td><%= u.name %></td>
    <td><%=u.email %></td>
  </tr>
<% end %>
</table>
